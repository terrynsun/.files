URxvt.perl-ext-common: font-size,default,matcher
URxvt.perl-ext: default,matcher
urxvt.perl-lib: /usr/lib/urxvt/perl/

! Click-to-open URLs
urxvt.urlLauncher: /usr/bin/firefox
urxvt.matcher.button: 1

URxvt*scrollBar: false

! Define fonts
URxvt*font: xft:Inconsolata:size=11
URxvt*boldFont:xft:Inconsolata:style=Bold:size=11
URxvt*italicFont:xft:Inconsolata:style=Oblique:size=11
URxvt*boldItalicFont:xft:Inconsolata:style=Bold Oblique:size=11

